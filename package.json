{
  "name": "queueue",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "env TAILWIND_MODE='watch' vite",
    "build": "NODE_ENV=production vite build && cp _redirects dist/",
    "zeus": "zeus schema.gql ./src --ts && echo 'type Opaque<Type, Token = unknown> = Type & { readonly __opaque__: Token };' > src/zeus/index.new.ts; cat src/zeus/index.ts | sed -E 's/\\[\"([a-zA-Z]+)\"\\]:(any|unknown)/\\[\"\\1\"\\]:Opaque<string, \"\\1\">/' >> src/zeus/index.new.ts; mv src/zeus/index.new.ts src/zeus/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^17.0.12",
    "@types/react-dom": "^17.0.8",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "autoprefixer": "^10.2.6",
    "graphql-zeus": "^3.0.8",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5",
    "vite": "^2.3.8",
    "vite-plugin-imagemin": "^0.3.2"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.18.1"
  }
}
