    "autoprefixer": "^10.2.3",
    "graphql-zeus": "^2.8.6",
    "parcel": "^2.0.0-nightly.577",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
